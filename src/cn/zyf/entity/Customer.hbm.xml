<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	
	<class name="cn.zyf.entity.Customer" table="customer">
		<id name="custId">
			<generator class="assigned"/>
		</id>
		
		<property name="custName"/>
		<property name="custGender"/>
		<property name="custBirth"/>
		<property name="custNative"/>
		<property name="custEdu"/>
		<property name="custSchool"/>
		<property name="custTel"/>
		<property name="custEmail"/>
		<property name="custJonIntention"/>
		<property name="custSource"/>
		<property name="custHA"/>
		<property name="custComment"/>
		<property name="custStatus"/>
		<property name="custTime"/>
		<property name="year"/>
		<property name="month"/>
		<property name="day"/>
		
	</class>
	
</hibernate-mapping>    